{
    "name":"serviceinfoadn/url",
    "version": "1.0.2",
    "description":"Class for handling and manipulating URLs",
    "type":"library",
    "autoload":{
        "psr-4":{
            "Adn\\Url\\": "src/Adn/Url"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adn\\Url\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0"
    },
    "suggest": {
        "psr/http-message": "For using the Psr7Uri class implementing Psr\\Http\\Message\\UriInterface"
    },
    "require-dev": {
        "phpunit/phpunit": "^4|^5|^6|^7|^8|^9|^10|^11",
        "psr/http-message": "^1.0 || ^2.0"
    }
}

